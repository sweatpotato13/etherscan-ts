{
    "name": "etherscan-ts",
    "version": "0.0.5",
    "description": "Ethersdcan API library with typescript",
    "main": "./dist/src/index.js",
    "types": "./dist/src/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/sweatpotato13/etherscan-ts.git"
    },
    "bugs": {
        "url": "https://github.com/sweatpotato13/etherscan-ts/issues"
    },
    "homepage": "https://github.com/sweatpotato13/etherscan-ts",
    "files": [
        "dist"
    ],
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "yarn run eslint --fix"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "prepare-commit-msg": "devmoji -e",
            "commit-msg": "if git-branch-is dev; then commitlint -E HUSKY_GIT_PARAMS; fi"
        }
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.0",
        "@commitlint/config-conventional": "^16.2.1",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "devmoji": "^2.3.0",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jest": "^26.1.1",
        "git-branch-is": "^4.0.0",
        "husky": "7.0.4",
        "jest": "^27.5.1",
        "lint-staged": "^12.3.4",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "standard-version": "^9.3.2",
        "ts-jest": "^27.1.3",
        "ts-node-dev": "^1.1.8",
        "tsutils": "^3.21.0",
        "typescript": "^4.5.5"
    },
    "scripts": {
        "dev": "ts-node-dev src/main.ts --trace-sync-io",
        "test": "jest",
        "lint": "eslint \"{src,test}/**/*.ts\" --fix",
        "prettier": "prettier --write --config .prettierrc ./src///*.ts && prettier --write --config .prettierrc ./test///*.ts",
        "build": "tsc",
        "prebuild": "rimraf ./dist",
        "release": "standard-version",
        "release:major": "standard-version -- --release-as major",
        "release:minor": "standard-version -- --release-as minor",
        "release:patch": "standard-version -- --release-as patch",
        "release:version": "standard-version -- --release-as"
    },
    "author": "CuteWisp <sweatpotato13@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "dotenv": "^16.0.0",
        "node-fetch": "^3.2.0",
        "tslib": "^2.3.1"
    },
    "volta": {
        "node": "12.18.3",
        "npm": "6.14.5"
    },
    "packageManager": "yarn@3.1.1"
}
